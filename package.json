{
  "name": "ractio",
  "version": "1.0.0",
  "description": "A sample radio player built with React.js - the clean commits provide a step by step setup guide",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "node server",
    "watch": "webpack --watch",
    "dev": "concurrently --kill-others --prefix \"[{name}]\" --names \"server,webpack\" \"npm start\" \"npm run watch\"",
    "open": "opn http://localhost:3000",
